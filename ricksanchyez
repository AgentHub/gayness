--functions

function RemoveDoors()
	for i,v in pairs(workspace.Doors:GetDescendants()) do
		if  v:IsA ("Part") and v.name == 'Door' then
			v:Destroy() end end end 
			
function AntiCheat()
   while wait(2) do
	for i,v in pairs(workspace:GetDescendants()) do 
		if v.name == "AntiOoB" then v:Destroy() end end end
end

function InfiniteAmmo()
    	for i,v in next,getgc(true)do
		if type(v)=='table'and rawget(v,'ammo')then
			v.ammo = math.huge
			v.stored_ammo = math.huge
		end end end 
		
function BringVest()
	for _,EventGiver in pairs(workspace.AREA51:GetDescendants()) do 
		if EventGiver.name == "Giver" 
			and EventGiver.Parent.name == RoomName then 
			firetouchinterest(Player.Character.HumanoidRootPart,EventGiver, 0) 
			end end end  

function GiveAllGuns()
	for _,GunGiver in pairs(game:GetService("Workspace").Weapons:GetDescendants()) do 
		if GunGiver.name == "Hitbox" 
		then firetouchinterest(Player.Character.HumanoidRootPart,GunGiver, 0) end end end

function GetPapers()
	for _,PaperGiver in pairs(workspace.AREA51:GetDescendants()) do 
		if PaperGiver.name == "TouchInterest"
			and PaperGiver.Parent.name == "Paper"
		then firetouchinterest(Player.Character.HumanoidRootPart,PaperGiver.Parent, 0) end end end

function WheelchairKillers()
while wait(2) do
	for i,v in pairs(Workspace.Killers:GetDescendants()) do 
		if v.name == 'Zombie' and v.parent.parent.name == 'Killers' then 
			v:destroy()
		end end end end
		
